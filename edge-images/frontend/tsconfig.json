{
  "compilerOptions": {
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "module": "es2015",
    "esModuleInterop": true,
    "importHelpers": true,
    "sourceMap": true,
    "baseUrl": ".",
    "types": ["@types/node", "@nuxt/types", "./types/vue", "vuetify"],
    "pretty": true,
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"],
      "*": ["./types"]
    },
    "target": "es5",
    "lib": ["esnext", "esnext.asynciterable", "dom.iterable", "dom"],
    "allowJs": true,
    "strict": true,
    "noEmit": true,
    "noImplicitAny": false
  }
}
