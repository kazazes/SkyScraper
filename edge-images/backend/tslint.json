{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest"
    ],
    "jsRules": {},
    "rules": {
        "promise-function-async": true,
        "await-promise": true,
        "no-floating-promises": true,
        "quotemark": {
            "options": "double"
        }
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
            "src/generated/**/*"
        ]
    }
}
