{
  "name": "skyscraper-vue",
  "version": "0.1.0",
  "private": true,
  "description": "Control and manage your Skyscraper.",
  "author": {
    "email": "peter@sibylvision.com",
    "name": "Peter Kazazes"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
