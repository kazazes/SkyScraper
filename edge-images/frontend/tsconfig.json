{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "noImplicitAny": false,
    "lib": ["esnext", "esnext.asynciterable", "dom"],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "paths": {
      "@/*": ["./*"],
      "~/*": ["./*"]
    },
    "sourceMap": true,
    "strict": true,
    "target": "esnext",
    "types": ["@types/node", "@nuxt/types", "vuetify", "./types/vue"]
  },
  "exclude": ["node_modules"]
}
